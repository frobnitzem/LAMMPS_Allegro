#!/bin/bash
#SBATCH -N 2
#SBATCH -t 00:20:00
#SBATCH -J MD
#SBATCH -A mat281
#SBATCH -o %j.out
#SBATCH -e %j.err

infile=lammps.in

module load ums
module load ums031/allegro

export OMP_NUM_THREADS=1
srun --ntasks-per-node 8 -c 7 \
    lmp -k on g 8 -sf kk -pk kokkos newton on neigh half -in $infile
